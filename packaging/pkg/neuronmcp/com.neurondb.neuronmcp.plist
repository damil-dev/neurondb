<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.neurondb.neuronmcp</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/neurondb-mcp</string>
    </array>
    <key>RunAtLoad</key>
    <false/>
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>
    <key>StandardOutPath</key>
    <string>/var/log/neuronmcp/neuronmcp.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/neuronmcp/neuronmcp.error.log</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>NEURONDB_MCP_CONFIG</key>
        <string>/etc/neuronmcp/mcp-config.json</string>
    </dict>
    <key>WorkingDirectory</key>
    <string>/var/lib/neuronmcp</string>
    <key>UserName</key>
    <string>neuronmcp</string>
</dict>
</plist>


