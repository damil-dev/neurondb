{
  "version": "1.0.0.beta",
  "architecture": "amd64",
  "neurondb": {
    "postgresql_versions": ["16", "17", "18"],
    "gpu_backends": "rocm",
    "compute_mode": "rocm",
    "build_args": {
      "CXXFLAGS": "",
      "CFLAGS": "",
      "LDFLAGS": ""
    },
    "cuda": {
      "enabled": false,
      "path": "",
      "version": ""
    },
    "rocm": {
      "enabled": true,
      "path": "/opt/rocm",
      "version": "5.7"
    },
    "metal": {
      "enabled": false
    }
  },
  "neuronagent": {
    "build_args": {
      "CGO_ENABLED": "0",
      "GOOS": "linux",
      "ldflags": "-w -s"
    }
  },
  "neuronmcp": {
    "build_args": {
      "CGO_ENABLED": "0",
      "GOOS": "linux",
      "ldflags": "-w -s"
    }
  }
}

