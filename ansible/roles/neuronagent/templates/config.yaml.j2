server:
  host: "{{ neuronagent_host }}"
  port: {{ neuronagent_port }}
  read_timeout: {{ neuronagent_read_timeout }}
  write_timeout: {{ neuronagent_write_timeout }}

database:
  host: "{{ neuronagent_db_host }}"
  port: {{ neuronagent_db_port }}
  database: "{{ neuronagent_db_name }}"
  user: "{{ neuronagent_db_user }}"
  password: "{{ neuronagent_db_password }}"
  max_open_conns: {{ neuronagent_db_max_open_conns }}
  max_idle_conns: {{ neuronagent_db_max_idle_conns }}
  conn_max_lifetime: {{ neuronagent_db_conn_max_lifetime }}

auth:
  api_key_header: "Authorization"

logging:
  level: "{{ neuronagent_log_level }}"
  format: "{{ neuronagent_log_format }}"

